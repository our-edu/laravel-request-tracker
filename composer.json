{
    "name": "our-education/laravel-request-tracker",
    "description": "Event-driven Laravel package for tracking user access patterns with daily summaries, detailed journeys, and module-level analytics",
    "version": "2.1.2",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "request", "tracker", "http", "logging", "middleware"],
    "homepage": "https://github.com/our-edu/laravel-request-tracker",
    "support": {
        "issues": "https://github.com/our-edu/laravel-request-tracker/issues",
        "source": "https://github.com/our-edu/laravel-request-tracker"
    },
    "authors": [{
        "name": "Our Education",
        "role": "Developer"
    }],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "OurEdu\\RequestTracker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OurEdu\\RequestTracker\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OurEdu\\RequestTracker\\RequestTrackerServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}