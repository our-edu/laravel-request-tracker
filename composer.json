{
  "name": "our-edu/laravel-request-tracker",
  "description": "Track incoming requests lifecycle and store request traces in DB (auto installable).",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "OurEdu\\RequestTracker\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "OurEdu\\RequestTracker\\RequestTrackerServiceProvider"
      ]
    }
  },
  "require": {
    "php": ">=8.0",
    "illuminate/support": "^9|^10|^11"
  }
}
